
public class ArrayExample {

	public static void main(String[] args) {
		int a1 [] = {1, 2, 3, 4, 5};
		
		printArray(a1);
		

	}

	private static void printArray(int[] theArray) {
		for(int i = 0; i<theArray.length;i++)
		{
			System.out.print(theArray[i]+" ");
			
			
		}
		System.out.println();
	}

}
