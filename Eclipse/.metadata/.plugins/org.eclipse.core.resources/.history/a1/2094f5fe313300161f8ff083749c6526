
public class MagicSquare 
{
	
private int rows;
private int columns;
private int [][] square;

MagicSquare()
{
	rows = 3;
	columns = 3;
	square = new int [rows][columns];
	
}
MagicSquare(int a, int b)
{
	rows = a;
	columns = b;
	square = new int [rows][columns];
}

public int size()
{
	return square.length;
}
public void add(int c, int d, int val)
{
	square[c][d] = val;
}
}
